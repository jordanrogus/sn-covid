        <css>/* Black Background w/ White Text */
* {
	background-color: $background-color !important;
  color: $default-font-color;
}

/* Center Text */
h2, .text-muted {
	text-align: center;
}

/* Hide Borders */
.b, .alert-info-border {
	border: none;
}

/* Ensure Asterisk Colors Stay Red */
.fa-asterisk {
	color: $button-color !important;
}

#sc_cat_item {
	display: block;
  margin-left: auto;
  margin-right: auto;
}

/* Button CSS */

.panel .panel- .b .wrapper-md, .sc-item-error-messages{
	display: none;
}

.sc-fixed{
	position: absolute;
  top: 150px;
  right: 0;
}
	
.form-group button {
	width: 225px;
	height: 50px;
	display:inline-block;
	border:0.16em solid rgba(255,255,255,0);
	border-radius:2em;
	box-sizing: border-box;
	text-decoration:none;
	font-weight: bold;
	color: $default-font-color;
	text-align:center;
	transition: all 0.2s;
	background-color: $button-color !important;
	font-size: 1.5em;
  position: absolute;
  top: 0;
  right: 0;
}

.form-group button:hover {
	border-color: rgba(255,255,255,1);
	padding-right: 24px;
	padding-left: 8px;
}

.form-group button:after {
	content: '»';
	position: relative;
	opacity: 0;  
	top: -2px;
	right: -25px;
	transition: 0.5s;
  }
  
.form-group button:hover:after {
	opacity: 1;
	right: -15px;
  }</css>
